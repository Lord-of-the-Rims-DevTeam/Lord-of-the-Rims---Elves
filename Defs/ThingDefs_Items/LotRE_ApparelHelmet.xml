<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <ThingDef ParentName="LotRE_ArmorHelmetMakeableBase">
    <defName>LotRE_ElfApparel_BattleHelmet</defName>
    <label>ñoldorin-elf helmet</label>
    <description>A part of the Elven battle armor set used by the skilled Ñoldor, or deep-elves, named for their wisdom.</description>
    <techLevel>Medieval</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Pawn/Elf/Apparel/Helmet/nackbladElvenHelmetDisplay</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costStuffCount>15</costStuffCount>
    <stuffCategories>
      <li>Leathery</li>
    </stuffCategories>
	<costList>
      <Steel>20</Steel>
      <Silver>1</Silver>
      <Gold>1</Gold>
	</costList>	
    <statBases>
      <WorkToMake>4000</WorkToMake>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>2.7</Mass>
      <ArmorRating_Blunt>0.25</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.5</ArmorRating_Sharp>
      <ArmorRating_Heat>0.5</ArmorRating_Heat>
      <Insulation_Cold>-3</Insulation_Cold>
      <Insulation_Heat>3</Insulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.04</MoveSpeed>
    </equippedStatOffsets>
		<generateCommonality>2</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Elf/Apparel/Helmet/nackbladElvenHelmet</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
        <li>LotR_ElfMilitary</li>
      </tags>
      <defaultOutfitTags>
        <li>LotR_ElfWarrior</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>

  <ThingDef ParentName="LotRE_ArmorHelmetMakeableBase">
    <defName>LotRE_ElfApparel_WoodElfCrown</defName>
    <label>elf crown</label>
    <description>A crown crafted in the shape of twigs and leaves to symbolize the woodland domain.</description>
