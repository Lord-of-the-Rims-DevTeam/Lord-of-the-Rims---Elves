    <costStuffCount>15</costStuffCount>
    <stuffCategories>
      <li>Woody</li>
      <li>Metallic</li>
    </stuffCategories>
	<costList>
      <Gold>2</Gold>
      <Silver>2</Silver>
	</costList>	
    <statBases>
      <WorkToMake>8000</WorkToMake>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>1</Mass>
      <ArmorRating_Blunt>0.15</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.25</ArmorRating_Sharp>
      <ArmorRating_Heat>0.25</ArmorRating_Heat>
      <Insulation_Cold>-3</Insulation_Cold>
      <Insulation_Heat>3</Insulation_Heat>
      <SellPriceFactor>2.5</SellPriceFactor>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.3</SocialImpact>
    </equippedStatOffsets>
		<generateCommonality>0.01</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Elf/Apparel/Helmet/sheidulaElfCrown</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
        <li>LotR_ElfKing</li>
      </tags>
	  </apparel>
  </ThingDef>

