<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <ThingDef ParentName="ArmorHelmetMakeableBase">
    <defName>LotRE_ElfApparel_BattleHelmet</defName>
    <label>ñoldorin-elf helmet</label>
    <description>A part of the Elven battle armor set used by the skilled Ñoldor, or deep-elves, named for their wisdom.</description>
    <techLevel>Medieval</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>10</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Pawn/Elf/Apparel/Helmet/nackbladElvenHelmetDisplay</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costStuffCount>15</costStuffCount>
    <stuffCategories>
      <li>Leathery</li>
    </stuffCategories>
	<costList>
      <Steel>20</Steel>
      <Silver>1</Silver>
      <Gold>1</Gold>
	</costList>	
    <statBases>
      <WorkToMake>9000</WorkToMake>
      <MaxHitPoints>130</MaxHitPoints>
      <Mass>1.1</Mass>
      <StuffEffectMultiplierArmor>0.82</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.25</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.1</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2.0</EquipDelay>
    </statBases>
		<generateCommonality>2</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Elf/Apparel/Helmet/nackbladElvenHelmet</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
		  <tags>
            <li>LotR_ElfMilitary</li>
          </tags>
    </apparel>
  </ThingDef>

